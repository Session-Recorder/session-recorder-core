		"morgan": "~1.9.1",
		"nedb": "^1.8.0",
		"request-country": "^0.1.5",
		"request-ip": "^2.1.3",
		"shortid": "^2.2.16"
	},
	"devDependencies": {
		"@types/cookie-parser": "^1.4.2",
		"@types/cors": "^2.8.10",
		"@types/express": "^4.17.11",
		"@types/http-errors": "^1.8.0",
		"@types/lodash": "^4.14.168",
